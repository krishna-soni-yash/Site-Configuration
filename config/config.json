{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "site-config-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/siteConfig/SiteConfigApplicationCustomizer.js",
          "manifest": "./src/extensions/siteConfig/SiteConfigApplicationCustomizer.manifest.json"
        }
      ]
    },
    "hide-banner-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/hideBanner/HideBannerWebPart.js",
          "manifest": "./src/webparts/hideBanner/HideBannerWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "SiteConfigApplicationCustomizerStrings": "lib/extensions/siteConfig/loc/{locale}.js",
    "HideBannerWebPartStrings": "lib/webparts/hideBanner/loc/{locale}.js"
  }
}
